<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="urjc.mswl.intentactivities"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="10" />
    // Give permission to call
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="urjc.mswl.intentactivities.IntentActivities"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="urjc.mswl.intentactivities.A1"
            android:label="@string/title_activity_a1" >
        </activity>
        <activity
            android:name="urjc.mswl.intentactivities.A2"
            android:label="@string/title_activity_a2" >
        </activity>
        <activity
            android:name="urjc.mswl.intentactivities.A3"
            android:label="@string/title_activity_a3" >
        </activity>
    </application>

</manifest>